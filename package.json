{
  "name": "js-libs",
  "private": true,
  "license": "LGPL-3.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "find . -type d '(' -name '.turbo' -o -name 'dist' -o -name 'node_modules' ')' -prune -exec rm -rf {} \\;",
    "dev": "turbo run dev",
    "format": "turbo run format",
    "lint": "turbo run lint",
    "prepare": "husky install",
    "push": "turbo run build lint test && changeset version && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@douglasneuroinformatics/eslint-config": "workspace:*",
    "@turbo/gen": "^1.10.16",
    "bun-types": "^1.0.7",
    "eslint": "^8.52.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "turbo": "^1.10.16",
    "typescript": "^5.2.2"
  }
}
