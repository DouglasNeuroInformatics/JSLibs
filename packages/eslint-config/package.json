{
  "name": "@douglasneuroinformatics/eslint-config",
  "version": "3.0.0",
  "description": "Linting configurations for DNP projects written in TypeScript/JavaScript",
  "type": "module",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "license": "LGPL-3.0",
  "exports": {
    ".": "./index.js"
  },
  "scripts": {
    "format": "prettier --write index.js",
    "lint": "eslint --fix index.js"
  },
  "dependencies": {
    "@joshunrau/eslint-config": "^3.2.4"
  },
  "peerDependencies": {
    "eslint": "8.x",
    "typescript": "5.x"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
