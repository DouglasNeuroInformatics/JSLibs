{
  "$schema": "https://json.schemastore.org/swcrc",
  "exclude": [".stories.tsx"],
  "jsc": {
    "baseUrl": "src",
    "parser": {
      "dynamicImport": true,
      "syntax": "typescript"
    },
    "paths": {
      "@/*": ["*"]
    },
    "target": "es2022"
  },
  "minify": false,
  "module": {
    "type": "es6"
  },
  "sourceMaps": true
}
